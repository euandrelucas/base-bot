generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @unique
  commands Int
}

model Cooldown {
  id        String @id
  command   String
  expiresAt BigInt
}